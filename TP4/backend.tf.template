# Configuration du backend S3 pour le state Terraform
terraform {
  backend "s3" {
    bucket         = "terraform-state-agvq0"
    key            = "global/s3/student_7/terraform.tfstate"
    region         = "eu-west-3"
    dynamodb_table = "terraform-up-and-running-locks"
    encrypt        = true

